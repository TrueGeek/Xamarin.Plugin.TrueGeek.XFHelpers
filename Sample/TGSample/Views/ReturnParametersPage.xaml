<?xml version="1.0" encoding="UTF-8"?>
<baseView:TGBasePage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:baseView="clr-namespace:TrueGeek.XFHelpers.Views;assembly=TrueGeek.XFHelpers"
    xmlns:vm="clr-namespace:TGSample.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="TGSample.Views.ReturnParametersPage">

    <ContentPage.BindingContext>
        <vm:ReturnParametersViewModel />
    </ContentPage.BindingContext>

    <StackLayout Padding="20" Spacing="20">

        <Button Text="Value 1" Command="{Binding SelectValueCommand}" CommandParameter="1" />

        <Button Text="Value 2" Command="{Binding SelectValueCommand}" CommandParameter="2" />

        <Button Text="Value 3" Command="{Binding SelectValueCommand}" CommandParameter="3" />

    </StackLayout>

</baseView:TGBasePage>